    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hotkeys-hook": "^4.6.1",
    "remix-utils": "^7.7.0",
    "tailwind-merge": "^2.5.5",
    "tailwindcss": "^3.4.15",
