    "@ethos/config": "^1.0.0",
    "@ethos/contracts": "^1.0.0",
    "@ethos/domain": "^1.0.0",
    "@ethos/echo-client": "^1.0.0",
    "@ethos/env": "^1.0.0",
    "@ethos/score": "^1.0.0",
    "@nivo/core": "^0.88.0",
    "@nivo/line": "^0.88.0",
    "@nivo/scales": "^0.88.0",
    "@privy-io/react-auth": "^1.93.0",
    "@privy-io/server-auth": "^1.16.6",
    "@privy-io/wagmi": "^0.2.12",
    "@radix-ui/react-focus-scope": "^1.1.0",
