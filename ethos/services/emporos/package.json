{
  "name": "@ethos/emporos",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "prebuild": "npm run generate-theme && prisma generate",
    "build": "remix vite:build",
    "predev": "npm run generate-theme && npm run prisma:migrate",
    "dev": "remix vite:dev",
    "generate-theme": "tsx scripts/generate-theme",
    "prestart": "tsx bootstrap.db.ssl.ts && npm run prisma:deploy",
    "start": "remix-serve ./build/server/index.js",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "typecheck": "tsc --noEmit -p tsconfig.json"
  },
  "dependencies": {
    "@ant-design/cssinjs": "^1.22.1",
